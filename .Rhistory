# Group data by sex
# Summarize mean body mass
penguins %>%
group_by(sex) %>%
summarize(mean_body_mass = mean(body_mass_g))
# Group data by sex
# Summarize mean body mass
penguins %>%
group_by(sex) %>%
summarize(mean_body_mass = round(mean(body_mass_g), 2))
# Group data by sex and species
# Summarize mean body mass
penguins %>%
group_by(species, sex) %>%
summarize(mean_body_mass = round(mean(body_mass_g), 2))
# Group data by island
# Summarize by counting the number of species
penguins %>%
group_by(species, island) %>%
summarize(mean_body_mass = round(mean(body_mass_g), 2))
# Group data by island
# Summarize by counting the number of species
penguins %>%
group_by(species, island) %>%
summarize(num_species = n_distinct(species))
# Group data by island
# Summarize by counting the number of species
penguins %>%
group_by(island) %>%
summarize(num_species = n_distinct(species))
# Arrange by increasing body mass
penguins %>% arrange(body_mass_g)
# Arrange by increasing body mass
penguins %>%
select(species, island) %>%
arrange(body_mass_g)
# Arrange by increasing body mass
penguins %>%
select(species, body_mass_g) %>%
arrange(body_mass_g)
